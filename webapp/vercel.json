{
  "version": 2,
  "builds": [
    { "src": "SMELO_bot.py", "use": "@vercel/python" },
    { "src": "webapp/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/ask", "dest": "SMELO_bot.py" },
    { "src": "/webhook", "dest": "SMELO_bot.py" },
    { "src": "/app", "dest": "/webapp/index.html" },
    { "src": "/(.*)", "dest": "/webapp/$1" }
  ]
}
